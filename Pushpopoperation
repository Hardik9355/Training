// changing element
{
    const arr = ["Hey","Hi"]
    arr[0] ="bye"
    console.log(arr)
}
// using push operation
{
    const arr = ["Hey","Hi"]
    arr.push("Hello")
    console.log(arr)
}
// using pop operations
{
    const arr = ["Hey","Hi"]
    arr.pop()
    console.log(arr)
}
{
    const name ={firstname:"Hardik" , lastname:"Garg"}
    console.log(name.firstname)
    console.log(name)
}
