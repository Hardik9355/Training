{
    // Performing operations on string and comparison operations
    let a ="Hardik";
    let b ="Garg";
    console.log(a+b)
    console.log(a+="Garg")
    let c =5
    console.log(a+=c)
    console.log(c+="5")
    console.log(a==b)
    console.log(a===b)
    console.log(a!=b)
    console.log(a>b)
    console.log(a<b)
    console.log(a>=b)
    console.log(a<=b)
    console.log(a==b? a:"Hardik")
    
}
